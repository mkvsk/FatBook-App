<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HeaderLogo">
        <attr name="logoHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="StepProgressBar">
        <attr name="inactiveDotColor" format="color" />
        <attr name="activeDotColor" format="color"/>
        <attr name="inactiveDotIcon" format="reference"/>
        <attr name="activeDotIcon" format="reference"/>
        <attr name="dotSize" format="dimension"/>
        <attr name="spacing" format="dimension"/>
        <attr name="activeDotIndex" format="integer"/>
        <attr name="cumulativeDots" format="boolean"/>
        <attr name="numberDots" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MyAccountHeaderView">
        <attr name="text" format="string" />
        <attr name="buttonIsVisible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextInputLayoutView" >
        <attr name="android:text" format="string" />
        <attr name="android:hint" format="string" />
        <attr name="android:textSize" format="dimension"  />
        <attr name="android:singleLine" format="boolean"  />
        <attr name="android:inputType" format="integer"  />
    </declare-styleable>

    <declare-styleable name="ViewSpinner" >
        <attr name="android:hint" format="string" />
    </declare-styleable>

    <declare-styleable name="ViewImage" >
        <attr name="android:src" format="integer" />
        <attr name="android:adjustViewBounds" format="boolean"  />
        <attr name="android:scaleType" format="integer"  />
    </declare-styleable>

    <declare-styleable name="ViewButtonWithProgress">
        <attr name="android:text" format="string"  />
        <attr name="android:textSize" format="dimension"  />
    </declare-styleable>

    <declare-styleable name="ViewCommonButton">
        <attr name="android:text" format="string"  />
        <attr name="android:enabled" format="boolean"  />
        <attr name="android:clickable" format="boolean"  />
    </declare-styleable>

    <declare-styleable name="ViewText">
        <attr name="android:text" format="string"  />
        <attr name="android:textSize" format="dimension"  />
        <attr name="android:clickable" format="boolean"  />
        <attr name="android:maxLines" format="integer"  />
        <attr name="android:gravity"/>
        <attr name="android:breakStrategy"/>
        <attr name="android:lineSpacingMultiplier" format="float"  />
        <attr name="underline_margin_top" format="dimension"  />
    </declare-styleable>

    <declare-styleable name="ViewEditText">
        <attr name="android:text" format="string" />
        <attr name="android:hint" format="string" />
        <attr name="android:textSize" format="dimension"  />
        <attr name="android:singleLine" format="boolean"  />
        <attr name="android:inputType" format="integer"  />
        <attr name="android:focusableInTouchMode" format="boolean"  />
        <attr name="android:cursorVisible" format="boolean"  />
        <attr name="android:enabled" format="boolean"  />
        <attr name="android:maxLength" format="integer"  />
    </declare-styleable>

    <declare-styleable name="ViewSearch">
        <attr name="android:text" format="string" />
        <attr name="android:hint" format="string" />
        <attr name="android:textSize" format="dimension"  />
        <attr name="android:singleLine" format="boolean"  />
        <attr name="android:inputType" format="integer"  />
        <attr name="android:focusableInTouchMode" format="boolean"  />
        <attr name="android:cursorVisible" format="boolean"  />
        <attr name="android:enabled" format="boolean"  />
        <attr name="android:maxLength" format="integer"  />
        <attr name="textPaddingStart" format="dimension"  />
        <attr name="textPaddingEnd" format="dimension"  />
        <attr name="hideSearchButtons" format="boolean"  />
    </declare-styleable>

    <declare-styleable name="ViewDatePicker">
        <attr name="android:text" format="string" />
        <attr name="android:hint" format="string" />
    </declare-styleable>

    <declare-styleable name="ViewTicketParam">
        <attr name="showArrow" format="boolean" />
        <attr name="paramIcon" format="integer" />
        <attr name="android:hint" format="string" />
    </declare-styleable>

    <declare-styleable name="ViewIokiParam">
        <attr name="showUnderline" format="boolean" />
        <attr name="android:hint" format="string" />
    </declare-styleable>

    <declare-styleable name="ViewFloatingButton" >
        <attr name="android:src" format="integer" />
        <attr name="android:enabled" format="boolean"  />
    </declare-styleable>

</resources>